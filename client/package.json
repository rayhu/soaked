{
    "name": "soaked",
    "version": "0.0.2",
    "private": true,
    "author": "Ray Hu (rayhu007@gmail.com)",
    "description": "A soaked client to establish websocket bridge",
    "homepage": "https://soaked.hulaorui.com",
    "repository": "https://github.com/rayhu/soaked",
    "main": "./dist/electron/main.js",
    "scripts": {
        "start": "electron .",
        "dev": "electron main.js --verbose localhost:1110 & node src/watcher.js",
        "dev2": "node .electron-vue/dev-runner.js",
        "all": "npm-run-all lint pkg test & run-p watcher",
        "pkg": "pkg src/client.js --out-path ./builds",
        "test": "electron main.js --verbose localhost:1110",
        "watcher": "node src/watcher.js",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build2": "node .electron-vue/build.js && electron-builder",
        "build:dir": "node .electron-vue/build.js && electron-builder --dir",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
        "test:unit": "vue-cli-service test:unit",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
        "vuelint": "vue-cli-service lint",
        "lint": "eslint --ignore-path ../.gitignore . --fix && prettier --write . "
    },
    "build": {
        "productName": "clientdesktop",
        "appId": "com.example.yourapp",
        "directories": {
            "output": "build"
        },
        "files": [
            "dist/electron/**/*"
        ],
        "dmg": {
            "contents": [
                {
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        },
        "mac": {
            "icon": "build/icons/icon.icns"
        },
        "win": {
            "icon": "build/icons/icon.ico"
        },
        "linux": {
            "icon": "build/icons"
        }
    },
    "license": "ISC",
    "engines": {
        "node": ">=0.8.9"
    },
    "dependencies": {
        "core-js": "^3.6.4",
        "vue-electron": "^1.0.6",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1",
        "vuex-electron": "^1.0.0",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.1",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "^4.17.1",
        "http-errors": "~1.6.3",
        "js-yaml": "^3.13.1",
        "mime-types": "latest",
        "morgan": "~1.9.1",
        "optimist": "latest",
        "pug": "^2.0.4",
        "uuid": "^7.0.3",
        "uuid-encoder": "^1.2.0",
        "valid-url": "^1.0.9",
        "ws": "^7.2.3",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.19",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-plugin-typescript": "~4.3.0",
        "@vue/cli-plugin-unit-jest": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "1.0.0-beta.31",
        "ajv": "^6.5.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "babel-minify-webpack-plugin": "^0.3.1",
        "cfonts": "^2.1.2",
        "chalk": "^2.4.1",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^5.1.6",
        "css-loader": "^0.28.11",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^8.2.5",
        "electron-debug": "^1.5.0",
        "electron-devtools-installer": "^2.2.4",
        "babel-eslint": "^8.2.3",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "typescript": "~3.8.3",
        "vue-template-compiler": "^2.6.11",
        "electron-builder": "^20.19.2",
        "eslint": "^4.19.1",
        "eslint-friendly-formatter": "^4.0.1",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-html": "^4.0.3",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.8.0",
        "eslint-plugin-standard": "^3.1.0",
        "mini-css-extract-plugin": "0.4.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "multispinner": "^0.2.1",
        "node-loader": "^0.6.0",
        "node-sass": "^4.9.2",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^15.2.4",
        "vue-style-loader": "^4.1.0",
        "webpack-cli": "^3.0.8",
        "webpack": "^4.15.1",
        "webpack-dev-server": "^3.1.4",
        "webpack-hot-middleware": "^2.22.2",
        "webpack-merge": "^4.1.3",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "pkg": "^4.4.7",
        "prettier": "2.0.4"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.1",
        "utf-8-validate": "^5.0.2"
    }
}
