{
    "name": "soaked",
    "version": "1.0.0",
    "description": "A soaked client to establish websocket bridge",
    "homepage": "https://soaked.hulaorui.com",
    "repository": "https://github.com/rayhu/soaked",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "node src/client.js --verbose localhost:1110 & node src/watcher.js",
        "all": "npm-run-all lint build test & run-p watcher",
        "pkg": "pkg src/client.js --out-path ./builds",
        "test": "node src/client.js --verbose localhost:1110 wss://soaked.hulaorui.com:9999",
        "watcher": "node src/watcher.js",
        "lint": "eslint --ignore-path ../.gitignore . --fix && prettier --write . "
    },
    "author": "Ray Hu",
    "license": "ISC",
    "engines": {
        "node": ">=0.8.9"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "^4.17.1",
        "http-errors": "~1.6.3",
        "js-yaml": "^3.13.1",
        "mime-types": "latest",
        "morgan": "~1.9.1",
        "optimist": "latest",
        "pug": "^2.0.4",
        "uuid": "^7.0.3",
        "uuid-encoder": "^1.2.0",
        "valid-url": "^1.0.9",
        "ws": "^7.2.3",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "electron": "^8.2.5",
        "electron-builder": "^22.6.0",
        "eslint": "^6.8.0",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "pkg": "^4.4.7",
        "prettier": "2.0.4"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.1",
        "utf-8-validate": "^5.0.2"
    }
}
